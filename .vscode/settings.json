{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "next.config.ts": ".prettier*, eslint*, components*, postcss*, tsconfig*, next-env.d.ts",
    },
    "symbols.files.associations": {
        "*.reducer.ts": "redux-reducer",
        "*.saga.ts": "redux-effects",
        "*.state.ts": "redux-facade",
        "*.types.ts": "redux-selector",
        "*.store.ts": "redux-actions"
    },
    "symbols.folders.associations": {
        "src/redux": "folder-redux-actions",
        "redux/reducers": "folder-redux-reducer",
        "redux/sagas": "folder-redux-effects",
        "redux/states": "folder-redux-facade",
        "redux/types": "folder-redux-selector"
    },
    "symbols.hidesExplorerArrows": true
}